            <all-aspect type="bool">true</all-aspect> <!-- all or rear-only -->
            <max-fire-range-nm type="double">12</max-fire-range-nm>
            <FCS-field-deg type="int">50</FCS-field-deg>
            <seeker-field-deg type="int">75</seeker-field-deg>
            <max-g type="int">35</max-g>
            <thrust-lbf-stage-1 type="double">3000</thrust-lbf-stage-1>
            <thrust-lbf-stage-2 type="double">0.0</thrust-lbf-stage-2>
            <stage-1-duration-sec type="double">5.23</stage-1-duration-sec>
            <stage-2-duration-sec type="double">0.0</stage-2-duration-sec>
            <weight-launch-lbs type="double">231</weight-launch-lbs>
            <weight-warhead-lbs type="double">16.3</weight-warhead-lbs>
            <weight-fuel-lbm type="double">50</weight-fuel-lbm>
            <drag-coeff type="double">0.43</drag-coeff>
            <cross-section-sqft type="double">0.12</cross-section-sqft> <!-- sq ft -->
            <arming-time-sec type="double">1.4</arming-time-sec>
            <min-speed-for-guiding-mach type="double">0.8</min-speed-for-guiding-mach>
            <self-destruct-time-sec type="double">43</self-destruct-time-sec>
            <seeker-angular-speed-dps type="double">30</seeker-angular-speed-dps> <!-- deg/sec guess-->
            <lock-on-sun-deg type="double">5</lock-on-sun-deg>
            <loft-altitude type="int">0</loft-altitude>
            <min-fire-range-nm type="double">0.001</min-fire-range-nm>
            <rail type="bool">true</rail>
            <rail-length-m type="double">2.667</rail-length-m>
            <rail-point-forward type="bool">true</rail-point-forward>
            <max-report-distance type="double">65</max-report-distance>
            <class type="string">A</class>
            <fire-msg type="string">Fox 2</fire-msg>
            <DLZ type="bool">true</DLZ>
            <DLZ-optimal-alt-feet type="double">25000</DLZ-optimal-alt-feet>
            <DLZ-optimal-closing-mach type="double">2.0</DLZ-optimal-closing-mach>
            <seeker-beam-width-deg type="double">4</seeker-beam-width-deg>
            <reaquire type="bool">true</reaquire>
            <lock-on-after-launch type="bool">true</lock-on-after-launch>
            <auto-switch-target-allowed type="bool">true</auto-switch-target-allowed>
            <prowl-flight type="string">gyro-pitch</prowl-flight>
            <switch-time-sec type="double">5</switch-time-sec>
            <coolable type="bool">true</coolable>
            <cool-time type="double">5</cool-time>
            <cool-duration type="int">3600</cool-duration>
            <warm-detect-range-nm type="double">6.5</warm-detect-range-nm>
            <detect-range-nm type="double">9.7</detect-range-nm>
            <ready-time type="double">0.5</ready-time>
        </r-73>
