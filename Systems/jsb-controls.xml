<?xml version="1.0" encoding="UTF-8"?>
 <flight_control name="FCS: T-50">

  <channel name="Pitch">
   <summer name="Pitch Trim Sum">
      <input>fcs/elevator-cmd-norm</input>
      <input>fcs/pitch-trim-cmd-norm</input>
      <clipto>
        <min> -1 </min>
        <max>  1 </max>
      </clipto>
   </summer>
   
   <pure_gain name="Target Pitch Rate">
      <input>fcs/pitch-trim-sum</input>
      <gain>-60</gain>
   </pure_gain>
   
   <pure_gain name="Pitch Rate">
      <input>velocities/q-rad_sec</input>
      <gain>-57.3</gain>
   </pure_gain>
   
   <summer name="Pitch Rate Error">
      <input>fcs/target-pitch-rate</input>
      <input>fcs/pitch-rate</input>
   </summer>
   
   <integrator name="Pitch Error Integrator">
      <input>fcs/pitch-rate-error</input>
      <c1>0.25</c1>
   </integrator>
   
   <scheduled_gain name="Scaled Pitch Error">
      <input>fcs/pitch-rate-error</input>
      <table>
        <independentVar lookup="row">velocities/ve-kts</independentVar>
        <tableData>
           0          -0.20
           100        -0.10
           200        -0.08
           500        -0.06
           1000       -0.01
        </tableData>
      </table>
   </scheduled_gain>
   
   <scheduled_gain name="Scaled Pitch Integrator">
      <input>fcs/pitch-error-integrator</input>
      <table>
        <independentVar lookup="row">velocities/ve-kts</independentVar>
        <tableData>
           0          -0.20
           100        -0.10
           200        -0.08
           500        -0.06
           1000       -0.01
        </tableData>
      </table>
   </scheduled_gain>
   
   <summer name="Pitch Output">
      <input>fcs/scaled-pitch-error</input>
      <input>fcs/scaled-pitch-integrator</input>
      <clipto>
         <min>-1.0</min>
         <max>1.0</max>
      </clipto>
   </summer>

   <aerosurface_scale name="Elevator Control">
      <input>fcs/pitch-output</input>
      <range>
        <min> -0.35 </min>
        <max>  0.35 </max>
      </range>
      <output>fcs/elevator-pos-rad</output>
   </aerosurface_scale>

   <aerosurface_scale name="elevator normalization">
      <input>fcs/elevator-pos-rad</input>
      <domain>
        <min> -0.35 </min>
        <max>  0.35 </max>
      </domain>
      <range>
        <min> -1 </min>
        <max>  1 </max>
      </range>
      <output>fcs/elevator-pos-norm</output>
   </aerosurface_scale>

  </channel>

  <channel name="Roll">
   <summer name="Roll Trim Sum">
      <input>fcs/aileron-cmd-norm</input>
      <input>fcs/roll-trim-cmd-norm</input>
      <clipto>
        <min> -1 </min>
        <max>  1 </max>
      </clipto>
   </summer>
   
   <pure_gain name="Target Roll Rate">
      <input>fcs/roll-trim-sum</input>
      <gain>270</gain>
   </pure_gain>
   
   <pure_gain name="Roll Rate">
      <input>velocities/p-rad_sec</input>
      <gain>-57.3</gain>
   </pure_gain>
   
   <summer name="Roll Rate Error">
      <input>fcs/target-roll-rate</input>
      <input>fcs/roll-rate</input>
   </summer>
   
   <integrator name="Roll Error Integrator">
      <input>fcs/roll-rate-error</input>
      <c1>0.25</c1>
   </integrator>
   
   <scheduled_gain name="Scaled Roll Error">
      <input>fcs/roll-rate-error</input>
      <table>
        <independentVar lookup="row">velocities/ve-kts</independentVar>
        <tableData>
           0          0.15
           100        0.08
           200        0.06
           500        0.05
           1000       0.04
        </tableData>
      </table>
   </scheduled_gain>
   
   <scheduled_gain name="Scaled Roll Integrator">
      <input>fcs/roll-error-integrator</input>
      <table>
        <independentVar lookup="row">velocities/ve-kts</independentVar>
        <tableData>
           0          0.10
           100        0.07
           200        0.05
           500        0.04
           1000       0.03
        </tableData>
      </table>
   </scheduled_gain>
   
   <summer name="Roll Output">
      <input>fcs/scaled-roll-error</input>
      <input>fcs/scaled-roll-integrator</input>
      <clipto>
         <min>-1.0</min>
         <max>1.0</max>
      </clipto>
   </summer>

   <aerosurface_scale name="Left Aileron Control">
      <input>fcs/roll-output</input>
      <range>
        <min> -0.35 </min>
        <max>  0.35 </max>
      </range>
      <output>fcs/left-aileron-pos-rad</output>
   </aerosurface_scale>

   <aerosurface_scale name="Right Aileron Control">
      <input>fcs/roll-output</input>
      <range>
        <min> -0.35 </min>
        <max>  0.35 </max>
      </range>
      <output>fcs/right-aileron-pos-rad</output>
   </aerosurface_scale>

   <aerosurface_scale name="left aileron normalization">
      <input>fcs/left-aileron-pos-rad</input>
      <domain>
        <min> -0.35 </min>
        <max>  0.35 </max>
      </domain>
      <range>
        <min> -1 </min>
        <max>  1 </max>
      </range>
      <output>fcs/left-aileron-pos-norm</output>
   </aerosurface_scale>

   <aerosurface_scale name="right aileron normalization">
      <input>fcs/right-aileron-pos-rad</input>
      <domain>
        <min> -0.35 </min>
        <max>  0.35 </max>
      </domain>
      <range>
        <min> -1 </min>
        <max>  1 </max>
      </range>
      <output>fcs/right-aileron-pos-norm</output>
   </aerosurface_scale>

  </channel>

  <channel name="Yaw">
   <summer name="Rudder Command Sum">
      <input>fcs/rudder-cmd-norm</input>
      <input>fcs/yaw-trim-cmd-norm</input>
      <clipto>
        <min> -0.35 </min>
        <max>  0.35 </max>
      </clipto>
   </summer>
   
   <pure_gain name="Target Yaw Rate">
      <input>fcs/rudder-command-sum</input>
      <gain>15</gain>
   </pure_gain>
   
   <pure_gain name="Yaw Rate">
      <input>velocities/r-rad_sec</input>
      <gain>57.3</gain>
   </pure_gain>
   
   <summer name="Yaw Rate Error">
      <input>fcs/target-yaw-rate</input>
      <input>fcs/yaw-rate</input>
   </summer>
   
   <integrator name="Yaw Error Integrator">
      <input>fcs/yaw-rate-error</input>
      <c1>0.25</c1>
   </integrator>
   
   <scheduled_gain name="Scaled Yaw Error">
      <input>fcs/yaw-rate-error</input>
      <table>
        <independentVar lookup="row">velocities/ve-kts</independentVar>
        <tableData>
           0          0.15
           100        0.07
           200        0.05
           500        0.03
           1000       0.02
        </tableData>
      </table>
   </scheduled_gain>
   
   <scheduled_gain name="Scaled Yaw Integrator">
      <input>fcs/yaw-error-integrator</input>
      <table>
        <independentVar lookup="row">velocities/ve-kts</independentVar>
        <tableData>
           0          0.20
           100        0.10
           200        0.08
           500        0.05
           1000       0.03
        </tableData>
      </table>
   </scheduled_gain>
   
   <summer name="Yaw Output">
      <input>fcs/scaled-yaw-error</input>
      <input>fcs/scaled-yaw-integrator</input>
      <clipto>
         <min>-1.0</min>
         <max>1.0</max>
      </clipto>
   </summer>

   <aerosurface_scale name="Rudder Control">
      <input>fcs/yaw-output</input>
      <range>
        <min> -0.35 </min>
        <max>  0.35 </max>
      </range>
      <output>fcs/rudder-pos-rad</output>
   </aerosurface_scale>

   <aerosurface_scale name="rudder normalization">
      <input>fcs/rudder-pos-rad</input>
      <domain>
        <min> -0.35 </min>
        <max>  0.35 </max>
      </domain>
      <range>
        <min> -1 </min>
        <max>  1 </max>
      </range>
      <output>fcs/rudder-pos-norm</output>
   </aerosurface_scale>

  </channel>

  <channel name="Flaps">
   <kinematic name="Flaps Control">
     <input>fcs/flap-cmd-norm</input>
     <traverse>
       <setting>
          <position>  0 </position>
          <time>      0 </time>
       </setting>
       <setting>
          <position> 15 </position>
          <time>      4 </time>
       </setting>
       <setting>
          <position> 30 </position>
          <time>      3 </time>
       </setting>
     </traverse>
     <output>fcs/flap-pos-deg</output>
   </kinematic>

   <aerosurface_scale name="flap normalization">
      <input>fcs/flap-pos-deg</input>
      <domain>
        <min>  0 </min>
        <max> 30 </max>
      </domain>
      <range>
        <min> 0 </min>
        <max> 1 </max>
      </range>
      <output>fcs/flap-pos-norm</output>
   </aerosurface_scale>

  </channel>

  <channel name="Landing Gear">
   <kinematic name="Gear Control">
     <input>gear/gear-cmd-norm</input>
     <traverse>
       <setting>
          <position> 0 </position>
          <time>     0 </time>
       </setting>
       <setting>
          <position> 1 </position>
          <time>     5 </time>
       </setting>
     </traverse>
     <output>gear/gear-pos-norm</output>
   </kinematic>

  </channel>

  <channel name="Speedbrake">
   <kinematic name="Speedbrake Control">
     <input>fcs/speedbrake-cmd-norm</input>
     <traverse>
       <setting>
          <position> 0 </position>
          <time>     0 </time>
       </setting>
       <setting>
          <position> 1 </position>
          <time>     1 </time>
       </setting>
     </traverse>
     <output>fcs/speedbrake-pos-norm</output>
   </kinematic>

  </channel>

 </flight_control>
