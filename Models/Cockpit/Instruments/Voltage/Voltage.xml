<?xml version="1.0" encoding="UTF-8"?>
<!-- V-1 Voltmeter -->
<PropertyList>
	<path>v-1.gltf</path>
	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>glass</object-name>
	</effect>
	<animation>
		<type>rotate</type>
		<object-name>v-1_needle</object-name>
		<property>fdm/jsbsim/electric/sources/voltmeter</property>
		<interpolation>
			<entry>
				<ind>0.0</ind>
				<dep>-120</dep>
			</entry>
			<entry>
				<ind>30.0</ind>
				<dep>120</dep>
			</entry>
		</interpolation>
		<axis>
			<x>0.0</x>
			<y>0.0</y>
			<z>-1.0</z>
		</axis>
	</animation>
</PropertyList>
