<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<path>hud2.ac</path>
	<animation>
		<type>select</type>
		<object-name>HUD.Glass</object-name>
		<condition>
			<equals>
				<value>0</value>
				<value>1</value>
			</equals>
		</condition>
	</animation>
	<effect>
		<object-name>HUD.Display</object-name>
		<inherits-from>Aircraft/Su-57/Models/Effects/display/display</inherits-from>
	</effect>
	<animation>
		<type>touch</type>
		<visible>true</visible>
		<object-name>HUD.Display</object-name>
		<action>
			<touch>0</touch>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>hud.hudBtClick(cmdarg().getNode("x").getValue(), cmdarg().getNode("y").getValue())</script>
			</binding>
		</action>
	</animation>
</PropertyList>
