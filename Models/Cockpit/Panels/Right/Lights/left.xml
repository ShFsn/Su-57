<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<path>Aircraft/Su-57/Models/Cockpit/Parts/knob1.ac</path>
	<animation>
		<type>knob</type>
		<object-name>knob</object-name>
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>left-lights-bug</tooltip-id>
				<label>Lights Left Side</label>
			</binding>
		</hovered>
		<property>fdm/jsbsim/electric/switches/lights/left</property>
		<factor>180</factor>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>-1</z>
		</axis>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>fdm/jsbsim/electric/switches/lights/left</property>
				<factor>0.02</factor>
				<min>0</min>
				<max>1</max>
				<wrap>false</wrap>
			</binding>
		</action>
	</animation>
</PropertyList>
