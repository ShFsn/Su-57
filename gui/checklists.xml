<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<checklist>
		<title>Engine Startup</title>  
		<item>
			<name>Battery 1</name>
			<value>ON</value>
			<condition>
				<equals>
					<property>fdm/jsbsim/electric/switches/battery1</property>
					<value>1</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/electric/switches/battery1</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>Battery 2</name>
			<value>ON</value>
			<condition>
				<equals>
					<property>fdm/jsbsim/electric/switches/battery2</property>
					<value>1</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/electric/switches/battery2</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>APU</name>
			<value>ON</value>
			<condition>
				<equals>
					<property>fdm/jsbsim/electric/switches/apu</property>
					<value>1</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/electric/switches/apu</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>APU-Generator</name>
			<value>ON</value>
			<condition>
				<equals>
					<property>fdm/jsbsim/electric/switches/apu-gen</property>
					<value>1</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/electric/switches/apu-gen</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>Select Engine 1</name>
			<value>1</value>
			<condition>
				<equals>
					<property>fdm/jsbsim/electric/switches/select-engine</property>
					<value>0</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/electric/switches/select-engine</property>
				<value>0</value>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/electric/switches/select-mode</property>
				<value>2</value>
			</binding>
		</item>
		<item>
			<name>Engine Starter</name>
			<value>Activate</value>
			<condition>
				<equals>
					<property>controls/engines/engine[0]/starter</property>
					<value>1</value>
				</equals>
			</condition>
			<binding>
				<command>nasal</command>
				<script>libraries.engineStart()</script>
			</binding>
		</item>
		<item>
			<name>Engine 1 Cutoff</name>
			<value>OFF</value>
			<condition>
				<equals>
					<property>controls/engines/engine[0]/cutoff</property>
					<value>0</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>controls/engines/engine[0]/cutoff</property>
				<value>0</value>
			</binding>
		</item>
		<item>
			<name>AC-Generator 1</name>
			<value>ON</value>
			<condition>
				<equals>
					<property>fdm/jsbsim/electric/switches/ac-gen1</property>
					<value>1</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/electric/switches/ac-gen1</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>Select Engine 2</name>
			<value>2</value>
			<condition>
				<equals>
					<property>fdm/jsbsim/electric/switches/select-engine</property>
					<value>2</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/electric/switches/select-engine</property>
				<value>2</value>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/electric/switches/select-mode</property>
				<value>2</value>
			</binding>
		</item>
		<item>
			<name>Engine Starter</name>
			<value>Activate</value>
			<condition>
				<equals>
					<property>controls/engines/engine[1]/starter</property>
					<value>1</value>
				</equals>
			</condition>
			<binding>
				<command>nasal</command>
				<script>libraries.engineStart()</script>
			</binding>
		</item>
		<item>
			<name>Engine 2 Cutoff</name>
			<value>OFF</value>
			<condition>
				<equals>
					<property>controls/engines/engine[1]/cutoff</property>
					<value>0</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>controls/engines/engine[1]/cutoff</property>
				<value>0</value>
			</binding>
		</item>
		<item>
			<name>AC-Generator 2</name>
			<value>ON</value>
			<condition>
				<equals>
					<property>fdm/jsbsim/electric/switches/ac-gen2</property>
					<value>1</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/electric/switches/ac-gen2</property>
				<value>1</value>
			</binding>
		</item>
		<item>
			<name>APU-Generator</name>
			<value>OFF</value>
			<condition>
				<equals>
					<property>fdm/jsbsim/electric/switches/apu-gen</property>
					<value>0</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/electric/switches/apu-gen</property>
				<value>0</value>
			</binding>
		</item>
		<item>
			<name>APU</name>
			<value>OFF</value>
			<condition>
				<equals>
					<property>fdm/jsbsim/electric/switches/apu</property>
					<value>0</value>
				</equals>
			</condition>
			<binding>
				<command>property-assign</command>
				<property>fdm/jsbsim/electric/switches/apu</property>
				<value>0</value>
			</binding>
		</item>
	</checklist>
</PropertyList>	  
